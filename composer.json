{
    "name": "andreinocenti/social-media-url-id-extractor-php",
    "description": "Exctract the id from a Social Media URL",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "andreinocenti/php-social-media-url-validator": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AndreInocenti\\SocialMediaUrlIdExtractor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/Pest.php"
        ]
    },
    "authors": [
        {
            "name": "Andre Inocenti",
            "email": "andreinocenti@gmail.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^2"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
